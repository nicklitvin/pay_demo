generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Users {
  userId     String @unique
  email      String 
}

model Transactions {
    userId String @unique
    date DateTime 
    amount Float
}

model Globals {
    type String @unique
    value String
}
