generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Users {
    email      String @unique
    transactions Transactions[]
}

model Transactions {
    email String @unique
    date DateTime 
    amount Float
    user Users @relation(fields: [email], references: [email])
}

model Globals {
    type String @unique
    value String
}
